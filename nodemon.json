{
    "restartable": "rs",
    "ignore": [
        ".git",
        "temp",
        "public",
        "node_modules"
    ],
    "verbose": true,
    "execMap": {
        "js": "node --harmony"
    },
    "watch": ["template", "static", "utils", "content"],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js css njk"
}
